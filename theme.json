{
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "color": "#CB7A5C",
          "name": "Brand",
          "slug": "brand"
        },
        {
          "color": "#747F64",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#5C757A",
          "name": "Tertiary",
          "slug": "tertiary"
        },
        {
          "color": "#E8E2D8",
          "name": "Accent",
          "slug": "accent"
        },
        {
          "color": "#434343",
          "name": "Text",
          "slug": "text"
        },
        {
          "color": "#F3F2EE",
          "name": "Background",
          "slug": "background"
        }
      ]
    },
    "layout": {
      "contentSize": "50rem",
      "wideSize": "62.5rem"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "defaultFontSizes": false,
      "fluid": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/montserrat/Montserrat-Regular.ttf"]
            },
            {
              "fontFamily": "Montserrat",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "600",
              "src": ["file:./assets/fonts/montserrat/Montserrat-SemiBold.ttf"]
            }
          ],
          "fontFamily": "'Montserrat', sans-serif",
          "name": "Montserrat",
          "slug": "montserrat"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Grown",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/grown/grown.ttf"]
            }
          ],
          "fontFamily": "'Grown', sans-serif",
          "name": "Grown",
          "slug": "grown"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "8px"
        },
        "color": {
          "background": "var(--wp--preset--color--tertiary)",
          "text": "var(--wp--preset--color--background)"
        },
        "css": "",
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--background)"
            }
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontStyle": "normal",
          "fontWeight": "600",
          "textTransform": "uppercase"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--tertiary)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "16px",
          "fontStyle": "normal",
          "fontWeight": "600",
          "textTransform": "uppercase"
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--grown)",
          "fontSize": "96px",
          "fontStyle": "normal",
          "fontWeight": "400",
          "letterSpacing": "3px"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--grown)",
          "fontSize": "64px",
          "fontStyle": "normal",
          "fontWeight": "400",
          "letterSpacing": "3px"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "48px",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "36px",
          "fontStyle": "normal",
          "fontWeight": "400"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "24px",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "16px",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "link": {
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--montserrat)",
          "fontSize": "16px",
          "fontStyle": "normal",
          "fontWeight": "400",
          "textDecoration": "underline"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--montserrat)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "400"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 3,
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}
